{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          "es2017-node7",
          "react"
        ],
        "plugins": ["babel-plugin-transform-es2015-arrow-functions", "transform-class-properties", "transform-async-to-generator", "transform-es2015-classes", "react-hot-loader/babel"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          "es2017-node7",
          "react"
        ],
        "plugins": ["babel-plugin-transform-es2015-arrow-functions", "transform-class-properties", "transform-async-to-generator", "transform-es2015-classes"],
        "sourceMaps": "none"
      }
    }
  }
}